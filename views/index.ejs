                     <div class="owl-prev"></div>
                     <div class="owl-next"></div>
                  </div>
               </div>
            </div>
         </div>
         <div class="video-player">
            <div class="video-camera">
               <span>View</span>
               <select>
                  <option>Camera 1</option>
                  <option>Camera 2</option>
                  <option>Camera 3</option>
               </select>
            </div>

            <video id="remoteViewLeft" style="width: 100%" autoplay></video>
            <video id="remoteViewRight" style="width: 100%" autoplay></video>
            <div id="threejsContainer" style="display:none; width: 100%;">
               <img id="btnFullscreen" style="position: absolute; width: 50px; height: auto;" src="/public/images/FullscreenButton1.jpg" />
            </div>
            <iframe src="https://www.youtube.com/embed/ZdP1qP8nfII" frameborder="0" gesture="media" allow="encrypted-media"
               allowfullscreen=""></iframe>
         </div>
      </div>

      <!-- Sidebar Right -->
      <div class="sidebar-right">
         <div class="chat">
            <div class="chat-tab">
               <ul>
                  <li id="btnGlobalChat" class="active">
                     <a>Global Chat</a>
                  </li>
                  <li id="btnRoomChat">
                     <a>Room Chat</a>
                  </li>
               </ul>
            </div>
            <div id="divGlobalChat" class="chat-content">
            </div>
            <div id="divRoomChat" style="display: none;" class="chat-content">
            </div>
            <div id="divChatTextArea" class="chat-box">
               <textarea id="txtChat" placeholder="Bir mesaj gönderin"></textarea>
               <button id="btnSendChat" class="btn btn-primary">
                  <i class="fa fa-paper-plane-o"></i>
               </button>
            </div>
         </div>
      </div>
   </div>

   <!-- Register Popup -->
   <div class="register-popup">
      <div class="container">
         <a class="hide-popup">
            <i class="fa fa-close"></i>
         </a>
         <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
         <button class="btn btn-success">Start the Race</button>
      </div>
   </div>

   <!-- Login -->
   <div class="modal" id="modalLogin" tabindex="-1">
      <div class="modal-dialog" role="document">
         <div class="modal-content">
            <div class="modal-header">
               <h5 class="modal-title">Login</h5>
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">×</span>
               </button>
            </div>
            <div class="modal-body">
               <div>
                  <div class="form-group">
                     <label for="txtLoginEmail" class="form-control-label">Email</label>
                     <input type="text" class="form-control" id="txtLoginEmail">
                  </div>
                  <div class="form-group">
                     <label for="txtLoginPassword" class="form-control-label">Password</label>
                     <input type="text" class="form-control" id="txtLoginPassword">
                  </div>
                  <button type="button" id="btnLogin" class="btn btn-primary">Login</button>

               </div>
            </div>
            <div class="modal-footer">
               Don't have an account yet?
               <a href="">Subscribe to Tracer® Access</a>
            </div>
         </div>
      </div>
   </div>

   <!-- Register -->
   <div class="modal" id="modalRegister" tabindex="-1">
      <div class="modal-dialog" role="document">
         <div class="modal-content">
            <div class="modal-header">
               <h5 class="modal-title">Register</h5>
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">×</span>
               </button>
            </div>
            <div class="modal-body">
               <div>
                  <div class="form-group">
                     <label for="txtRegisterUsername" class="form-control-label">Username</label>
                     <input type="text" class="form-control" id="txtRegisterUsername">
                  </div>
                  <div class="form-group">
                     <label for="txtRegisterPassword" class="form-control-label">Password</label>
                     <input type="text" class="form-control" id="txtRegisterPassword">
                  </div>
                  <div class="form-group">
                     <label for="txtRegisterEmail" class="form-control-label">Email Adres</label>
                     <input type="text" class="form-control" id="txtRegisterEmail">
                  </div>
                  <button type="button" class="btn btn-facebook">
                     <i class="fa fa-facebook-f fa-fw"></i>Facebook</button>
                  <button type="button" class="btn btn-plus">
                     <i class="fa fa-google-plus fa-fw"></i>Google Plus</button>
                  <button id="btnRegister" type="button" class="btn btn-primary">Register</button>

               </div>
            </div>
            <div class="modal-footer">
               Don't have an account yet?
               <a href="">Subscribe to Tracer® Access</a>
            </div>
         </div>
      </div>
   </div>

   <!-- Create Room -->
   <div class="modal" id="modalCreateRoom" tabindex="-1">
      <div class="modal-dialog" role="document">
         <div class="modal-content">
            <div class="modal-header">
               <h5 class="modal-title">Create Room</h5>
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">×</span>
               </button>
            </div>
            <div class="modal-body">
               <div>
                  <div class="form-group">
                     <label for="txtRoomName" class="form-control-label">Room Name</label>
                     <input type="text" class="form-control" id="txtRoomName" maxlength="20">
                  </div>
                  <!-- <div class="form-group">
                            <label for="txLoginEmail" class="form-control-label">Driver Count</label>
                            <div>
                                <span>2</span>
                                <input type="radio" name="radioDriverCount2">
                                <span>3</span>
                                <input type="radio" name="txLoginEmail">
                                <span>4</span>
                                <input type="radio" name="txLoginEmail">
                            </div>
                        </div> -->
                  <div class="form-group">
                     <label for="txtRoomPassword" class="form-control-label">Room Password</label>
                     <input type="text" class="form-control" id="txtRoomPassword" maxlength="20">
                  </div>
                  <button type="button" id="btnCreateRoom" class="btn btn-primary">Create Room</button>

               </div>
            </div>
         </div>
      </div>
   </div>

   <!-- Enter Room Password -->
   <div class="modal" id="modalRoomPassword" tabindex="-1">
      <div class="modal-dialog" role="document">
         <div class="modal-content">
            <div class="modal-header">
               <h5 class="modal-title">Enter Room Password</h5>
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">×</span>
               </button>
            </div>
            <div class="modal-body">
               <div>
                  <div class="form-group">
                     <label for="txtRoomPassword" class="form-control-label">Room Password</label>
                     <input type="text" class="form-control" id="txtJoinRoomPassword" maxlength="20">
                  </div>
                  <button type="button" id="btnJoinRoom" class="btn btn-primary">Join Room</button>
               </div>
            </div>
         </div>
      </div>
   </div>

   <script>
      $(document).ready(function () {
         $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
         });

         $('.shareSelector').socialShare({
            social: 'facebook,twitter,google,pinterest,linkedin',
            whenSelect: true,
            animation: 'launchpadReverse',
            selectContainer: '.shareSelector',
            blur: true
         });

         $("#track").owlCarousel({
            items: 6,
            autoPlay: 5000,
            itemsDesktop: [1199, 6],
            itemsDesktopSmall: [979, 2],
            itemsTablet: [768, 2],
            itemsMobile: [479, 2],
            responsive: true,
            pagination: true,
            navigationText: ["", ""],
            navigation: true
         });

         $('.hide-popup').on('click', function () {
            $('.register-popup').hide();
         });
      });



   </script>




