      <!-- Content -->
      <div class="content">
         <div class="content-nav">
            <a class="share-btn shareSelector">
               <i class="fa fa-share-alt fa-fw"></i>Share</a>
         </div>
         <div class="track-maps">
            <div id="track" class="owl-carousel owl-theme" style="opacity: 1; display: block;">
               <div class="owl-wrapper-outer">
                  <div class="owl-wrapper" style="width: 1080px; left: 0px; display: block; transition: all 1000ms ease; transform: translate3d(0px, 0px, 0px);">
                     <div class="owl-item" style="width: 90px;">
                        <a href="">
                           <img src="/public/images/map1.jpg">
                           <span>Harita-1</span>
                        </a>
                     </div>
                     <div class="owl-item" style="width: 90px;">
                        <a href="">
                           <img src="/public/images/map1.jpg">
                           <span>Harita-2</span>
                        </a>
                     </div>
                     <div class="owl-item" style="width: 90px;">
                        <a href="">
                           <img src="/public/images/map1.jpg">
                           <span>Harita-3</span>
                        </a>
                     </div>
                     <div class="owl-item" style="width: 90px;">
